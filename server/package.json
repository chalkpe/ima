{
  "name": "@ima/server",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias",
    "dev": "tsx watch src/index.ts",
    "generate": "prisma generate",
    "lint": "eslint . && tsc --noEmit",
    "prepare": "npx @fastify/secure-session > secret-key",
    "prestart": "prisma db push --skip-generate",
    "start": "node dist/index.js",
    "test": "jest",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit"
  },
  "dependencies": {
    "@fastify/jwt": "^8.0.1",
    "@fastify/passport": "^2.5.0",
    "@fastify/secure-session": "^7.5.1",
    "@fastify/websocket": "^10.0.1",
    "@soundxyz/redis-pubsub": "^4.1.2",
    "@trpc/server": "^11.0.0-rc.571",
    "@types/passport-strategy": "^0.2.38",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "iovalkey": "^0.1.0",
    "megalodon": "^10.0.2",
    "passport-strategy": "^1.0.0",
    "passport-twitter": "^0.1.5",
    "tsx": "^4.19.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@types/passport-twitter": "^1.0.40",
    "@types/ws": "^8.5.12",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-alias": "^1.2.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  }
}
